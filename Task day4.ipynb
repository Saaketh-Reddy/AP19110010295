    "def common(a,b):\n",
    "    flag = False\n",
    "    for i in a:\n",
    "        if (i in b):\n",
    "            flag = True\n",
    "        else:\n",
    "            pass\n",
    "    return flag\n",
    "\n",
    "a = [1,2,3,4,5]\n",
    "b = [4,5,6,7,8,9]\n",
    "result = common(a, b)\n",
    "print('Both lists have Common elements ::',result)\n",
